<html lang="de">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>{{ title|default('n/a') }}</title>
    {% block styles %}
        <link rel="stylesheet" href="{{ link('styles/app.css') }}">
        <link rel="icon" type="image/svg+xml" href="{{ link('favicon.svg') }}">
        <link rel="icon" type="image/png" href="{{ link('favicon.png') }}">
    {% endblock %}
</head>
<body class="d-flex flex-column justify-content-between">
    {% block body %}{% endblock %}
    {% block javascripts %}
        <script type="text/javascript" src="{{ link('app.js') }}"></script>
    {% endblock %}
</body>
</html>