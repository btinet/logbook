    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>{{ app.title|default('n/a') }}</title>
    {% block styles %}
        <link rel="stylesheet" href="{{ link('styles/app.css') }}">
    {% endblock %}
</head>
<body>
    {% block body %}{% endblock %}
